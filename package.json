{
  "name": "gemini-chatbot-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "build": "npm run build --workspace=frontend "build": "npm run build --workspace=backend","build": "npm run build --workspace=backend", npm run build --workspace=backend",
    "start": "npm start --workspace=backend",
    "dev": "concurrently \"npm:dev --workspace=backend\" \"npm:dev --workspace=frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
